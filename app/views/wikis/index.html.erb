<h1>Wikis</h1>

<div class="row">
    <div class="col-md-8">
        
        <%= link_to "New Wiki", new_wiki_path, class: "btn btn-success" %>
        

        <% @wiki.each do |wiki| %>
            <div class="media">
                <div class="media-body">
                    <h4 class="media-heading">
                        <%= link_to render_markdown(wiki.title), wiki %><% if wiki.private? %> - Private <% end %></h3>
                       <h5> WIKI ID: <%= wiki.user_id %> USER ID: <%= current_user.id %></h5>
                    </h4>
                </div>
            </div>
        <% end %>

    </div>
</div>